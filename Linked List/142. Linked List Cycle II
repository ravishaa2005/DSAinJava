public class Solution {
    public ListNode detectCycle(ListNode head) {
        ListNode slow=head;
        ListNode fast=head;
        ListNode temp=head;
        while(fast!=null){
            if(slow==null)return null;
            slow=slow.next;
            if(fast.next==null) return null;
            fast=fast.next.next;
            if(slow==fast){
                while(temp!=slow){
                    temp=temp.next;
                    slow=slow.next;
                }
                return slow;
            }
        }
        return null;
    }
}